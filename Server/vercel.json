{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node",
            "config": {
                "buildCommand": "npm install && npm run install"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js",
            "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS" ]
        }
    ]
}